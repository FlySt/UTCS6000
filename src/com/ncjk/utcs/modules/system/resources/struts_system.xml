<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.i18n.encoding" value="utf-8"></constant>
	<package name="struts2_system" extends="struts2" >
		<action name="systemParam_*" class="systemParamAction" method="{1}">
		<result name="success">/modules/systemConfig/paramConfig.jsp</result>	
		</action>
		<action name="netParam_*" class="netWorkParamAction" method="{1}">
		</action>
		<action name="pluginParam_*" class="pluginParamAction" method="{1}">
		</action>
	</package>
</struts>
