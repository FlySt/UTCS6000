	width: 220px;
	height: 100%;
	float: left;
	overflow: hidden;
	border-bottom: 1px solid #A8CAE5;
}
.right-panel {
	position: relative;
	height: 100%;
	float: left;
}

.left-down-panel {
	position: absolute;
	width: 99%; /* 100% lead to no right border */
	overflow: auto;
	bottom: 0px;
	border: 1px solid #A8CAE5;
}

.right-down-panel {
	position: absolute;
	width: 100%;
	bottom: 0px;
	overflow: hidden;
}

